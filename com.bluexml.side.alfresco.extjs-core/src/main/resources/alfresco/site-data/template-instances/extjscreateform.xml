<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
	<template-type>com/bluexml/extjs/extjsform</template-type>
	<properties>
		<pageFamily>ExtJS</pageFamily>
	</properties>
	
	<components>
		<component>
			<region-id>formPortlet</region-id>
			<url>/components/form?mimeType={mimeType}&amp;destination={destination}&amp;redirect={redirect}&amp;formId={formId}&amp;css={css}&amp;js={js}</url>
			
			<properties>
				<itemKind>type</itemKind>
				<itemId>{itemId}</itemId>
				<mode>create</mode>
				<showCaption>true</showCaption>
				<showSubmitButton>true</showSubmitButton>
				<showCancelButton>true</showCancelButton>
				<submitType>{submitType}</submitType>
			</properties>
			
		</component>
	</components>    
</template-instance>
