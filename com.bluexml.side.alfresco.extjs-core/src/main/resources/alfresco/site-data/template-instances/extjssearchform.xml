<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
	<template-type>com/bluexml/extjs/searchform</template-type>
	<properties>
		<pageFamily>ExtJS</pageFamily>
	</properties>
	
	<components>
		<component>
			<region-id>searchform</region-id>
			<url>/components/bluexml/search/advsearch</url>
			
			<properties>
				<itemKind>type</itemKind>
				<itemId>{itemId}</itemId>
				<mode>edit</mode>
				<formId>search</formId>
				<showCaption>{showCaption}</showCaption>
				<showSubmitButton>{showSubmitButton}</showSubmitButton>
				<showCancelButton>{showCancelButton}</showCancelButton>
				<single>{single}</single>
			</properties>
			
		</component>
	</components>   
</template-instance>
